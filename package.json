{
  "name": "use-vue-service",
  "version": "1.0.8",
  "description": "Use angular service in vue.",
  "scripts": {
    "tsc": "tsc -p tsconfig.json",
    "tsc:w": "tsc -p tsconfig.json -w",
    "rp": "rollup -c rollup.config.js",
    "rp:w": "npm run tsc:w | rollup -wc rollup.config.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint src --fix --ext js,ts,jsx,tsx,vue",
    "dev": "webpack serve",
    "build": "npm run tsc && npm run rp",
    "clean": "rm -rf dist/*",
    "postpublish": "git push origin --follow-tags",
    "prepublishOnly": "npm run clean && npm run build",
    "release": "standard-version -r",
    "release:first": "standard-version --first-release"
  },
  "keywords": [
    "angular",
    "vue",
    "service",
    "Ioc",
    "DI",
    "Dependency injection"
  ],
  "bin": {},
  "browser": "",
  "es2015": "./dist/index.es2015.js",
  "main": "./dist/index.cjs.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bugs": {
    "url": "https://github.com/huanggm/use-vue-service/issues"
  },
  "homepage": "https://github.com/huanggm/use-vue-service",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/huanggm/use-vue-service.git"
  },
  "author": "huanggm",
  "license": "MIT",
  "peerDependencies": {
    "reflect-metadata": "^0.1.13",
    "vue": "^3.0.0-rc.1",
    "vue-class-component": "^8.0.0-rc.1"
  },
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/preset-env": "^7.12.1",
    "@rollup/plugin-commonjs": "^15.1.0",
    "@rollup/plugin-node-resolve": "^9.0.0",
    "@rollup/plugin-replace": "^2.3.3",
    "@typescript-eslint/eslint-plugin": "^4.5.0",
    "@typescript-eslint/parser": "^4.5.0",
    "@vue/compiler-sfc": "^3.0.1",
    "@webpack-cli/serve": "^1.0.1",
    "babel-loader": "^8.1.0",
    "css-loader": "^5.0.0",
    "eslint": "^7.11.0",
    "eslint-config-prettier": "^6.13.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-vue": "^7.2.0",
    "file-loader": "^6.1.1",
    "html-webpack-plugin": "^4.5.0",
    "husky": "^4.3.0",
    "lint-staged": "^10.4.2",
    "prettier": "^2.1.2",
    "reflect-metadata": "^0.1.13",
    "rollup": "^2.29.0",
    "rollup-plugin-terser": "^7.0.2",
    "standard-version": "^9.0.0",
    "style-loader": "^2.0.0",
    "ts-loader": "^8.0.5",
    "tslib": "^2.0.2",
    "typescript": "^4.0.3",
    "vue": "^3.0.0-rc.1",
    "vue-loader": "^16.0.0-beta.4",
    "vue-router": "^4.0.0-alpha.13",
    "vuex": "^4.0.0-beta.2",
    "webpack": "^5.1.3",
    "webpack-cli": "^4.1.0",
    "webpack-dev-server": "^3.11.0"
  },
  "files": [
    "dist/**/*"
  ],
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ]
  },
  "jest": {
    "roots": [
      "<rootDir>/src"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts"
    ],
    "setupFiles": [
      "react-app-polyfill/jsdom"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/src/setupTests.ts"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
      "<rootDir>/src/**/*.{spec,test}.{js,jsx,ts,tsx}"
    ],
    "testEnvironment": "jest-environment-jsdom-fourteen",
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|ts|tsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "modulePaths": [],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ],
    "watchPlugins": [
      "jest-watch-typeahead/filename",
      "jest-watch-typeahead/testname"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,ts,jsx,tsx,vue}": [
      "eslint --fix --ext js,ts,jsx,tsx,vue src"
    ]
  }
}
